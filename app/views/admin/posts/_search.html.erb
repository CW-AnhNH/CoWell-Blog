<%= search_form_for @q, url: admin_posts_path do |f| %>
    <%= f.search_field :title_or_content_cont %>
    <%= f.label :Public %>
    <%= f.check_box :privacy_eq_any, { multiple: true }, 0, '' %>
    <%= f.label :Private %>
    <%= f.check_box :privacy_eq_any, { multiple: true }, 1, '' %>
    <%= f.submit "Search", class: "btn btn-outline-info" %>
<% end %>
